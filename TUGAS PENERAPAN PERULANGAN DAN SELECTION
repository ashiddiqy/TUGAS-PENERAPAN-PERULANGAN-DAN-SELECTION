#include <iostream>
using namespace std;

int main ()
{
	int angkaAwal = 1;
	int angkaAkhir = 999;
	
	for (int i = angkaAwal ; i <= angkaAkhir ; i++) 
	{
		int counterPrima = 0;
		for (int j = 1 ; j <= i ; j++) 
		{
			if (i % j == 0) 
			{
				counterPrima++;
			}
		}
		if (counterPrima == 2)
		{
			cout << i << " Adalah bilangan prima" <<endl;
		}
	}
}
